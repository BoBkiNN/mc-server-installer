{
  "$defs": {
    "DatapackManifest": {
      "properties": {
        "provider": {
          "discriminator": {
            "mapping": {
              "github": "#/$defs/GithubReleasesProvider",
              "github-actions": "#/$defs/GithubActionsProvider",
              "modrinth": "#/$defs/ModrinthProvider",
              "url": "#/$defs/DirectUrlProvider"
            },
            "propertyName": "type"
          },
          "oneOf": [
            {
              "$ref": "#/$defs/ModrinthProvider"
            },
            {
              "$ref": "#/$defs/GithubReleasesProvider"
            },
            {
              "$ref": "#/$defs/DirectUrlProvider"
            },
            {
              "$ref": "#/$defs/GithubActionsProvider"
            }
          ],
          "title": "Provider"
        },
        "asset_id": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Asset Id"
        }
      },
      "required": [
        "provider"
      ],
      "title": "DatapackManifest",
      "type": "object"
    },
    "DirectUrlProvider": {
      "description": "Downloads asset from specified url",
      "properties": {
        "url": {
          "format": "uri",
          "maxLength": 2083,
          "minLength": 1,
          "title": "Url",
          "type": "string"
        },
        "type": {
          "const": "url",
          "enum": [
            "url"
          ],
          "title": "Type",
          "type": "string"
        }
      },
      "required": [
        "url",
        "type"
      ],
      "title": "DirectUrlProvider",
      "type": "object"
    },
    "GithubActionsProvider": {
      "description": "Downloads artifact from github actions",
      "properties": {
        "repository": {
          "title": "Repository",
          "type": "string"
        },
        "branch": {
          "default": "master",
          "title": "Branch",
          "type": "string"
        },
        "workflow": {
          "title": "Workflow",
          "type": "string"
        },
        "name_pattern": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Name Pattern"
        },
        "type": {
          "const": "github-actions",
          "enum": [
            "github-actions"
          ],
          "title": "Type",
          "type": "string"
        }
      },
      "required": [
        "repository",
        "workflow",
        "type"
      ],
      "title": "GithubActionsProvider",
      "type": "object"
    },
    "GithubReleasesProvider": {
      "description": "Downloads asset from github",
      "properties": {
        "repository": {
          "title": "Repository",
          "type": "string"
        },
        "type": {
          "const": "github",
          "enum": [
            "github"
          ],
          "title": "Type",
          "type": "string"
        }
      },
      "required": [
        "repository",
        "type"
      ],
      "title": "GithubReleasesProvider",
      "type": "object"
    },
    "ModManifest": {
      "properties": {
        "provider": {
          "discriminator": {
            "mapping": {
              "github": "#/$defs/GithubReleasesProvider",
              "github-actions": "#/$defs/GithubActionsProvider",
              "modrinth": "#/$defs/ModrinthProvider",
              "url": "#/$defs/DirectUrlProvider"
            },
            "propertyName": "type"
          },
          "oneOf": [
            {
              "$ref": "#/$defs/ModrinthProvider"
            },
            {
              "$ref": "#/$defs/GithubReleasesProvider"
            },
            {
              "$ref": "#/$defs/DirectUrlProvider"
            },
            {
              "$ref": "#/$defs/GithubActionsProvider"
            }
          ],
          "title": "Provider"
        },
        "asset_id": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Asset Id"
        },
        "version": {
          "title": "Version",
          "type": "string"
        }
      },
      "required": [
        "provider",
        "version"
      ],
      "title": "ModManifest",
      "type": "object"
    },
    "ModrinthProvider": {
      "description": "Downloads asset from modrinth",
      "properties": {
        "project_id": {
          "title": "Project Id",
          "type": "string"
        },
        "channel": {
          "anyOf": [
            {
              "$ref": "#/$defs/VersionType"
            },
            {
              "type": "null"
            }
          ],
          "default": null
        },
        "version_is_id": {
          "default": false,
          "title": "Version Is Id",
          "type": "boolean"
        },
        "version_name_pattern": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Version Name Pattern"
        },
        "ignore_game_version": {
          "default": true,
          "title": "Ignore Game Version",
          "type": "boolean"
        },
        "type": {
          "const": "modrinth",
          "enum": [
            "modrinth"
          ],
          "title": "Type",
          "type": "string"
        }
      },
      "required": [
        "project_id",
        "type"
      ],
      "title": "ModrinthProvider",
      "type": "object"
    },
    "PaperCoreManifest": {
      "properties": {
        "type": {
          "const": "paper",
          "enum": [
            "paper"
          ],
          "title": "Type",
          "type": "string"
        },
        "build": {
          "anyOf": [
            {
              "const": "latest",
              "enum": [
                "latest"
              ],
              "type": "string"
            },
            {
              "type": "integer"
            }
          ],
          "title": "Build"
        }
      },
      "required": [
        "type",
        "build"
      ],
      "title": "PaperCoreManifest",
      "type": "object"
    },
    "PluginManifest": {
      "properties": {
        "provider": {
          "discriminator": {
            "mapping": {
              "github": "#/$defs/GithubReleasesProvider",
              "github-actions": "#/$defs/GithubActionsProvider",
              "modrinth": "#/$defs/ModrinthProvider",
              "url": "#/$defs/DirectUrlProvider"
            },
            "propertyName": "type"
          },
          "oneOf": [
            {
              "$ref": "#/$defs/ModrinthProvider"
            },
            {
              "$ref": "#/$defs/GithubReleasesProvider"
            },
            {
              "$ref": "#/$defs/DirectUrlProvider"
            },
            {
              "$ref": "#/$defs/GithubActionsProvider"
            }
          ],
          "title": "Provider"
        },
        "asset_id": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Asset Id"
        },
        "version": {
          "title": "Version",
          "type": "string"
        }
      },
      "required": [
        "provider",
        "version"
      ],
      "title": "PluginManifest",
      "type": "object"
    },
    "VersionType": {
      "enum": [
        "release",
        "beta",
        "alpha"
      ],
      "title": "VersionType",
      "type": "string"
    }
  },
  "properties": {
    "mc_version": {
      "title": "Mc Version",
      "type": "string"
    },
    "core": {
      "discriminator": {
        "mapping": {
          "paper": "#/$defs/PaperCoreManifest"
        },
        "propertyName": "type"
      },
      "oneOf": [
        {
          "$ref": "#/$defs/PaperCoreManifest"
        }
      ],
      "title": "Core"
    },
    "mods": {
      "default": [],
      "items": {
        "$ref": "#/$defs/ModManifest"
      },
      "title": "Mods",
      "type": "array"
    },
    "plugins": {
      "default": [],
      "items": {
        "$ref": "#/$defs/PluginManifest"
      },
      "title": "Plugins",
      "type": "array"
    },
    "datapacks": {
      "default": [],
      "items": {
        "$ref": "#/$defs/DatapackManifest"
      },
      "title": "Datapacks",
      "type": "array"
    }
  },
  "required": [
    "mc_version",
    "core"
  ],
  "title": "Manifest",
  "type": "object"
}